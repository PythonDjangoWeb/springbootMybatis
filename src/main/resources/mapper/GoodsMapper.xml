<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cwh.springbootMybatis.web.good.mapper.GoodMapper" >

	<select id="selectListGoods" parameterType="com.cwh.springbootMybatis.web.good.entity.Good" resultType="com.cwh.springbootMybatis.web.good.entity.Good">
		select * from goods 
			order by id DESC
	</select>
	
</mapper>